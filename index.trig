@base <https://djradon.github.io/ns/d/> .
@prefix sflo: <https://semanticflow.github.io/ns/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# import all data from the current "default dataset" distribution

<> a owl:NamedIndividual ;

    # default dataset
    sflo:has_default_dataset_series <_default> ;
    sflo:has_current_default_dataset_trig_distribution <_default/current/d-default.trig> ;

    # default catalog
    sflo:has_default_catalog_series <_catalog> ;
    sflo:has_current_default_catalog_trig_distribution <_catalog/current/d-catalog.trig> ;

    .


